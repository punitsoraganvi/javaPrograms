class P2
{
	public static void main(String[] args) 
	{
		int n = 5;
		for(int i=0; i<n; i++){
			int k=5;
			char ch='a';
			for(int j=0; j<n; j++){
				if(i<=j){
					if(i%2==0){	
					System.out.print(k-- +" ");
					}else{
					System.out.print(ch++ +" ");
					}
				}else{
					System.out.print("  ");
						
				}
			} 
			char ch1='e';
			for(int j=0; j<n; j++){
				if(i+j>=n-1){
					System.out.print(ch1-- + " ");
				}else{
					System.out.print("  ");
				}

			}
			System.out.println();
		}
	}
}
